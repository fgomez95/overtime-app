<h1>Audit Logs</h1>

<table class="table">
    <tr>
        <th>Id</th>
        <th>User</th>
        <th>Start Date</th>
        <th>End Date</th>
        <th>Status</th>
    </tr>
    
    <tbody id="audit_logs">
        <%= render @audit_logs %>
    </tbody>
    
    
</table>

<div id="paginator">
   <%= paginate @audit_logs, remote: true %> 
</div>
